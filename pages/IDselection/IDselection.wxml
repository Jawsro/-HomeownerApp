<view class="container">
  <!-- 组件引入 -->
  <header  my-property="实名认证"></header> 
  <view>
    <image src="http://gsh.5gzvip.idcfengye.com/assets/app/bg3.jpg" mode="widthFix"></image>
  </view>
  <view class="main">
    <view class="owner all-style" wx:for="{{idArray}}" wx:key='index' 
    style="background:url('http://gsh.5gzvip.idcfengye.com/assets/app/bg1.jpg') center center no-repeat;background-size: cover">
      <view class="text" >
        <view class="left">
          <text class="font-weight-600 font-20">我是{{item.text}}</text>
        </view>
        <navigator class="right font-16" url="{{item.url}}?identity={{item.identity}}" hover-class="none">
          <text class="margin-right">去认证</text>
          <text class="iconfont icon-gengduo"></text>
        </navigator>
      </view>
    </view>
    
  </view>
</view>
