<view class="container">
  <view class="top" wx:if='{{loginStatue}}'>
    <view class="top-img">
      <image src="http://api.5gzvip.idcfengye.com/assets/app/bg4.jpg" mode="widthFix"></image>
    </view>
    <view class="img2">
      <image src="http://api.5gzvip.idcfengye.com/assets/app/buzhidao.jpg" mode="widthFix"></image>
      <view class="login-btn font-weight-500 font-16" bindtap="goLogin">
        <text>注册/登录</text>
      </view>
    </view>
  </view>
  <view class="top" style="height:320rpx" wx:if='{{!loginStatue}}'>
    <!-- <view class="top-title" style="height:{{height}}px;position:fixed;top:{{top}}px;left:20px;right:0;color:#833833;font-weight:600;display:flex;justify-content: center;align-items: center;z-index:10">
      <view style="flex:0 0 90rpx;" class="font-18 font-weight-500">
