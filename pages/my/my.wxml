      <view class="wx-title font-14" style="flex:1">
        |筑造美好生活
      </view>
    </view> -->
    <view class="user-msg" wx:if='{{yemian}}'>
      <view class="head-img">
        <text class="iconfont icon-touxiang" style="color:#C7C7C7;font-size:60px" wx:if="{{userInfo.weixin_headimg==null?true:false}}"></text>
        <image src="{{userInfo.weixin_headimg}}" wx:if="{{headImg!=null?true:false}}" mode="widthFix" style="border-radius: 50%;"></image>
      </view>
      <view class="user-text">
        <view class="user-name font-20  font-weight-500">{{userInfo.nickname}}</view>
        <!-- <view class="grade font-12" 
          wx:if ="{{authenticationStatus == 'yes'? false:true}}">
          未实名认证</view>
        <view class="grade font-12" bindtap="goOwner" wx:if ="{{ authenticationStatus == 'yes'? true:false}}">已认证</view> -->
      </view>
    </view>
   
  </view>

  <view class="main">
    <!-- 钱包 -->
    <!-- <view class="wallet">
      <view class="wallet-title">
        <view class="wallet-title-left">
          <text class="font-18 font-weight-500" style="margin-right:40rpx">我的钱包</text>
          <text class="iconfont " style="font-size:55rpx;color:#000;"></text>
        </view>
        <view class="wallet-title-right">
          <text class="font-18 font-weight-500" style="margin-right:40rpx"></text>
          <text class="iconfont icon-gengduo" style="font-size:40rpx;color:#000;"></text>
        </view>
      </view>
    </view> -->
    <view class="functions">
      <!--汽车，车位，商铺 -->
      <view class="functions-item" 
        wx:for='{{functionList}}'
        wx:key='index'
        data-url="{{item.url}}"
        bindtap="goFunction">
        <view class="functions-name">
          <view class="functions-item-left">
            <text>{{item.name}}</text>
          </view>
          <text class="iconfont icon-gengduo" style="color:#A6A6A6"></text>
        </view>
      </view> 
    </view>
  </view>
</view>

