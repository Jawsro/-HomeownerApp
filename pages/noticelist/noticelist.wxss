.information{
  background: #fff;
  border-radius: 20rpx;
  margin-top:150rpx ;
  padding:30rpx;
}
.news-list{
  display: flex;
  justify-content: center;
  border-bottom: 1px solid #EBEBEB;
  padding:40rpx 0 ;
}
.news-list-right{
  flex: 0 0 200rpx;
}
.news-list-left{
  flex:1;
  padding-right:60rpx;
  position: relative;
}
.news-title {
  color:#443C38;
}
.time{
  color:#A6A6A7;
  position: absolute;
  bottom: 0;
}
.date{
  margin-right: 40rpx;
}
.news-list-right image{
  border-radius: 20rpx;
  height: 100%;
}
.more{
  color:#A7A7AA;
  text-align: center;
  margin-top:20rpx
}