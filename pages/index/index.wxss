.container{
  padding-bottom: 40rpx;
  width: 100%;
}
.change-village{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: #F7F2EF;
}
.change-village-main{
  width: 500rpx;
  padding: 20rpx;
  position: absolute;
  top: 58%;
  left: 50%;
  transform: translate(-50%, -50%);
  text-align: center;
 
}
.change-village-title{
  margin-bottom: 20rpx;
  font-weight: 600;
}
.village-item{
  padding: 10rpx 0;
}
.top{
  position: relative;
  width: 100%;
  height: 445rpx;
}
.subdistrict-list{
  padding:45rpx 0 0 35rpx;
  color:#000;
}
.img2{
  position: absolute;
  top: 164rpx;
  left: 30rpx;
  right: 30rpx;
}
.login-btn{
  position: absolute;
  top: 30rpx;
  right: 30rpx;
  border-radius: 50rpx;
  border: 1px solid #591814;
  padding:20rpx 0;
  width:206rpx;
  text-align: center;
  color: #591814;
}
.user-msg{
  display: flex;
  align-items: center;
  justify-content: center;
  position: absolute;
  top:162rpx;
  width: 100%;
}
.xiaoqu{
  position: absolute;
  left: 80rpx;
  width:400rpx !important;
  z-index:10;
}
.xiaoqu-name{
  color: #1D212D;
  font-weight: 600;
  letter-spacing: 2rpx;
}
.xiaoqu-floor{
  color: #1C222D;
  padding: 10rpx 0;
}
.houses{
  background-color: #fff;
  border-radius: 20rpx;
  position: absolute;
  top:105rpx;
  padding: 0 20rpx;
  width: 544rpx;
  box-shadow: 0 4rpx 8rpx 1rpx rgba(0,0,0,0.3);
}
.house-list{
  display: flex;
  align-items: center;
  padding: 30rpx 0rpx;
  border-bottom: 1rpx solid #F4F4F4;
}
.house-list-left{
  margin-right: 20rpx;
}

.house-msg{
  color:#7F402F;
  font-weight: 100;
}
.add-house{
  padding: 30rpx 0rpx;
}
.erweima{
  position: absolute;
  right: 80rpx;
  z-index:10
}
.doudou-img{
  position: absolute;
  bottom: 0rpx;
  width: 100%;
  height:60rpx ;z-index: 3;
}
.doudou-img image{
  height: 100%;
}
.bgcolor{
  position: absolute;
  bottom: -64rpx;
  width: 100%;
  height:126rpx ;
  background-color: #E9D8D0;
}
.classification{
  padding:24rpx 0 50rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  flex-wrap:wrap;
  background: linear-gradient(to bottom, #E9D8D0 0%,#DEC7B9 50%,#D9BDAD 100%);
  z-index: 9;
}
.classification-item{
  width: 25%;
  height: 120rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.classification-item-img{
  font-size: 74rpx;
  color: #7F402F;
  margin-bottom: 10rpx;
}
.classification-item-text{
  color: #7F402F;
}
.grid{
  background: #fff;
  margin: 10rpx 0;
  display: flex;
  flex-direction: row;
  align-items: center;
  flex-wrap:wrap;
  padding:30rpx 0;
  width: 100%;
}
.grid-item{
  width: 25%;
  height: 120rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin-bottom: 40rpx;
}
.grid-item-img{
  font-size: 78rpx;
  color: #C8A792;
}
.grid-item-text{
  color: #C8A792;
}
.module{
  display: flex;
  flex-wrap:wrap;
  padding:20rpx 30rpx;
  background: #fff;
}
.module .item{
  width:49%;
  margin-bottom: 10rpx;
}
.right{
  margin-right: 10rpx;
}
.module .item image{
  border-radius: 10rpx;
}
.community .list{
  margin-bottom: 30rpx;
}
.community .text{
  background-color: #fff;
  padding:20rpx 0 20rpx 30rpx
}
.notices{
  display: flex;
  align-items: center;
  justify-content: center;
}
.notices .left{
  flex: 1;
}
.notices .right{
  flex: 1;
  text-align: right;
}
